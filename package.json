{
  "name": "dou-parser-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "npm run dev --workspace=apps/web",
    "dev:desktop": "npm run dev --workspace=apps/desktop",
    "build:web": "npm run build --workspace=apps/web",
    "build:desktop": "npm run build --workspace=apps/desktop",
    "install:all": "npm install",
    "web": "npm run dev --workspace=apps/web",
    "mobile": "npm run start --workspace=apps/mobile",
    "desktop": "npm run start --workspace=apps/desktop"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "dependencies": {
    "axios": "^1.13.3",
    "react-native-html-parser": "^0.1.0"
  }
}